{
    "name": "mininglamp-solution",
    "version": "2.0.0",
    "description": "mininglamp-solution",
    "main": "index.js",
    "scripts": {
        "precommit": "cp build/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit",
        "prepush": "npm run lintall",
        "server": "nodemon server/index.js",
        "release": "standard-version",
        "postrelease": "git push --follow-tags origin develop",
        "start": "cross-env NODE_ENV=development webpack serve --progress --config ./build/webpack.config.js",
        "dev": "cross-env NODE_ENV=development node --max_old_space_size=4096 build/dev.js",
        "modifyHosts": "sudo node ./build/hosts-modify.js",
        "dll": "webpack --config build/webpack.dll.config.js --progress --profile",
        "build": "cross-env NODE_ENV=production node --max_old_space_size=4096 build/build.js",
        "deploy": "cross-env NODE_ENV=production webpack --progress --config ./build/webpack.config.js",
        "postdeploy": "node ./build/copy-to-test.js",
        "lintall": "sh build/pre-commit lintall || true",
        "stylelint": "stylelint './src/**/index.scss' --fix",
        "jsdoc": "jsdoc ./src/*.js --readme ./README.md -t ./node_modules/ink-docstrap/template -c ./conf.json",
        "test": "jest",
        "test:watch": "npm test -- --watch",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint": "eslint src --fix --ext .js,.jsx,.ts,.tsx ",
        "eslint": "eslint --ext .tsx,.ts --fix ./src",
        "prettier": "prettier -u -l  \"./src/**/*\"",
        "prettier:fix": "prettier -u --write -l \"./src/**/*\"",
        "createtypes": "npx ytt -c ytt.config.ts",
        "build:docker": "sh build/build-docker.sh"
    },
    "authors": [
        "anchao <pvfhv@qq.com>",
        "mizi <mizi.lin@miaozhen.com>"
    ],
